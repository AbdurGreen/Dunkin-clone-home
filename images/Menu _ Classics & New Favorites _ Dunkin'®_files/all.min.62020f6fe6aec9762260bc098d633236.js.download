"use strict";

var APP = (window.APP = window.APP || {});

APP.nutritionFilter = (function () {
  var dataUrl = "/bin/servlet/nutrition",
    pageLoad = true,
    nutritionAltText,
    mayContainFlag = false,
    sizeExist = false,
    allergens = ["eggs", "fish", "milk", "peanuts", "wheat", "soy", "treenuts", "sesame"],
