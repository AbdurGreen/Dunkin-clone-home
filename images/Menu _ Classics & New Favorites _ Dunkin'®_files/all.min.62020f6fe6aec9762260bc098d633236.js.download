            $formWrapper.hide();
            $confirmationWrapper.show();
            getConfirmation();
            $(window).scrollTop($('.form-pats-wrapper__confirmation').offset().top);
        }
        if (!isDDPerksUser && isUserLoggedIn) {
            showNonPerksUserMsg();
        }
