    };

    /**
     * interfaces to public functions
     */
    return {
        init: init,
        bindOnResize: bindOnResize,
        shellSignInFailure: shellSignInFailure,
        isPartnerPromoCookieCreated: isPartnerPromoCookieCreated.promise(),
