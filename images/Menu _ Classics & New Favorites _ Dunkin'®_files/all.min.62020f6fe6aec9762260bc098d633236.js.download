        $closestStore = $('.js-store-find-closest');
        var radius = $closestStore.data('radius');
        APP.store
            .getClosestStore(radius)
            .done(storeFound)
            .fail(storeNotFound)
            .always(function () {
                $closestStore.addClass('store-find-closest--show');
